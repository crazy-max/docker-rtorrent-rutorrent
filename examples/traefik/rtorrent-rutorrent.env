TZ=Europe/Paris
PUID=1000
PGID=1000

WAN_IP_CMD=false

AUTH_DELAY=0s
MEMORY_LIMIT=256M
UPLOAD_MAX_SIZE=16M
OPCACHE_MEM_SIZE=128
MAX_FILE_UPLOADS=50
REAL_IP_FROM=0.0.0.0/32
REAL_IP_HEADER=X-Forwarded-For
LOG_IP_VAR=http_x_forwarded_for
LOG_ACCESS=true

XMLRPC_AUTHBASIC_STRING=rTorrent XMLRPC restricted access
